#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n,i;
	unsigned long long factorial=1;
	printf("enter a number to find factorial:");
	scanf("%d",&n);
	if(n<0)
		printf("error!enter any positive number");
	else
	{
		for(i=1;i<=n;++i)
		{
			factorial*=i;
			
		}
		printf("factorial of  %d=%llu",n,factorial);
	}
	return 0;
	
	
}
